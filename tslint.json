{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "class-name": false,
    "interface-name": false,
    "member-ordering": false,
    "no-console": false,
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "trailing-comma": false,
    "variable-name": false,
    "indent": [
      true,
      "spaces",
      2
    ]
  },
  "rulesDirectory": []
}
